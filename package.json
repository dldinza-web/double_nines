{
  "name": "app",
  "private": "true",
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@hotwired/stimulus": "^3.1.0",
    "@hotwired/turbo-rails": "^7.2.0",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.9",
    "@mui/x-date-pickers-pro": "^5.0.4",
    "@rails/ujs": "^7.0.4",
    "esbuild": "^0.15.10",
    "graphql": "^16.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.55.0",
    "typescript": "^4.8.3"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets",
    "build:css": "sass ./app/assets/stylesheets/application.sass.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules",
    "rebuild": "npm run build && npm run build:css && npm run tsc && npm run lint",
    "tsc": "tsc --project tsconfig.json",
    "check-types": "tsc --project tsconfig.json --noEmit --watch --preserveWatchOutput",
    "lint": "eslint app/javascript/**/*.ts[x]",
    "test:frontend": "jest",
    "test:backend": "bundle exec rspec",
    "test": "npm run test:backend && npm run test:frontend",
    "test-env": "bundle exec rails s -e test -p 5002",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "e2e": "npm run rebuild && start-server-and-test test-env http://localhost:5002 cypress:open",
    "it": "npm run rebuild && start-server-and-test test-env http://localhost:5002 cypress:run",
    "start": "./bin/dev"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.0.2",
    "@types/node": "^16.11.59",
    "@types/rails__ujs": "^6.0.1",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^29.0.3",
    "cypress": "^10.10.0",
    "eslint": "^8.23.1",
    "eslint-config-preact": "^1.3.0",
    "eslint-plugin-react": "^7.31.8",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^18.2.0",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^29.0.1"
  },
  "jest": {
    "testEnvironment": "jsdom"
  }
}
